    <DataSet Name="SystemInformation">
      <Query>
        <DataSourceName>VisionDemo50</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ActivePeriod">
            <Value>=Parameters!ActivePeriod.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ActiveCompany">
            <Value>=Parameters!ActiveCompany.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
Period,
FirmName, 
MultiCompanyEnabled,
MultiCurrencyEnabled,
AccountPdStart,
AccountPdEnd, 
WBS1Length,
WBS2Length,
WBS3Length,
OrgLevels
 FROM CFGMainData left join CFGDates on CFGDates.Period = @ActivePeriod,
CFGFormat,
FW_CFGSystem
 WHERE CFGMainData.Company = @ActiveCompany</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>

  <Code>'************************************************************
' Used to divide numbers in an expression and avoid dividing by 0
'************************************************************
Function SafeDivide(ByVal numerator As Decimal, ByVal denominator As Decimal) As Decimal
        If (denominator = 0) Then
            Return 0
        Else
            Return numerator / denominator
        End If
 End Function

Public Function Period As String
If Month(Now())  &gt; 9 Then
	Return str(Year(Now())) &amp; Trim(str(Month(Now())))
Else
	Return str(Year(Now())) &amp; "0" &amp; Trim(str(Month(Now())))
End If
End Function

Public Function GetWeekStartDate(weekNumber As Integer, year As Integer) As Date
    Dim startDate As New DateTime(year, 1, 1)
    Dim weekDate As DateTime = DateAdd(DateInterval.WeekOfYear, weekNumber - 1, startDate)
    Return DateAdd(DateInterval.Day, (-weekDate.DayOfWeek) + 1, weekDate)
End Function

Public Function DateCalc(weeknmbr as Integer, wloc as string) as Date
	Dim CurWeekNmbr as Integer = DatePart(DateInterval.WeekofYear, Now())
	Dim CalcWeekStart as Date = DateAdd(DateInterval.WeekofYear,weeknmbr, Now())
	Dim CalcWeekSNmbr as Integer = DatePart(DateInterval.WeekOfYear, CalcWeekStart)
	Dim startDate As New DateTime(DatePart(DateInterval.Year, CalcWeekStart), 1, 1)
	Dim weekDate As DateTime = DateAdd(DateInterval.WeekOfYear, CalcWeekSNmbr  - 1, startDate)	
	Dim StartWeek as Date = DateAdd(DateInterval.Day, (-weekDate.DayOfWeek) + 1, weekDate)
	Dim EndWeek as Date = DateAdd(DateInterval.Day, (-weekDate.DayOfWeek) + 7, weekDate)
	If ucase(wloc)="S" Then
		Return StartWeek
	Else
		Return EndWeek
	End If 
End Function

Public Function DayCalc(weeknmbr as Integer, wloc as string) as Integer
	Dim CurWeekNmbr as Integer = DatePart(DateInterval.WeekofYear, Now())
	Dim CalcWeekStart as Date = DateAdd(DateInterval.WeekofYear,weeknmbr, Now())
	Dim CalcWeekSNmbr as Integer = DatePart(DateInterval.WeekOfYear, CalcWeekStart)
	Dim startDate As New DateTime(DatePart(DateInterval.Year, CalcWeekStart), 1, 1)
	Dim weekDate As DateTime = DateAdd(DateInterval.WeekOfYear, CalcWeekSNmbr  - 1, startDate)	
	Dim StartWeek as Date = DateAdd(DateInterval.Day, (-weekDate.DayOfWeek) + 1, weekDate)
	Dim EndWeek as Date = DateAdd(DateInterval.Day, (-weekDate.DayOfWeek) + 7, weekDate)
	Dim SWNumb as Integer = DateDiff(DateInterval.Day, Now(), StartWeek)+1
	Dim EWNumb as Integer = DateDiff(DateInterval.Day, Now(), EndWeek)+1
	If ucase(wloc)="S" Then
		Return SWNumb
	Else
		Return EWNumb 
	End If 
End Function	
